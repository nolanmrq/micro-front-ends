    "log-module": {
      "projectType": "library",
      "root": "projects/log-module",
      "sourceRoot": "projects/log-module/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/log-module/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/log-module/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/log-module/tsconfig.lib.json"
            }
          },
