              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "todo-module": {
      "projectType": "library",
      "root": "projects/todo-module",
      "sourceRoot": "projects/todo-module/src",
      "prefix": "lib",
      "architect": {
